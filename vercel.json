{
  "version": 2,
  "name": "llmai",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "functions": {
    "client/app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  }
}
